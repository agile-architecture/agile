{
  "name": "multieditor-ts",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@agile-ts/core": "file:.yalc/@agile-ts/core",
    "@agile-ts/multieditor": "file:.yalc/@agile-ts/multieditor",
    "@agile-ts/react": "file:.yalc/@agile-ts/react",
    "material-ui": "^0.20.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "typescript": "^4.4.3",
    "yup": "^0.32.9"
  },
  "devDependencies": {
